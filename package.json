{
  "name": "react-sandbox-v2",
  "version": "0.0.1",
  "description": "Updated version of the React-Sandbox with updated packages and methods",
  "main": "index.js",
  "repository": "https://github.com/abarcenas29/react-sandbox-v2",
  "license": "MIT",
  "scripts": {
    "git:submodules": "git submodule update --init --recursive",
    "build:prod": "rimraf ./build/* && cross-env NODE_ENV=production && webpack --config internals/webpack.config.js --mode production --progress",
    "postinstall": "rimraf node_modules/jest-runtime/node_modules/babel-core node_modules/jest-config/node_modules/babel-core && npm run git:submodules",
    "start:dev": "cross-env NODE_ENV=development && rimraf ./build/* && webpack-dev-server --config internals/webpack.config.js --mode development --open --hot",
    "test": "cross-env NODE_ENV=test && jest --colors"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.8.19",
    "final-form": "^4.18.6",
    "final-form-arrays": "^3.0.2",
    "graphql": "^14.5.8",
    "react": "^16.12.0",
    "react-async": "^10.0.0",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.3.3",
    "react-final-form-arrays": "^3.1.1",
    "react-helmet": "^5.2.1",
    "react-intl": "^3.11.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "autoprefixer": "^9.7.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.0",
    "sanitize.css": "^11.0.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import",
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": false
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "plugins": [
      "react"
    ],
    "globals": {
      "workbox": true,
      "self": true,
      "fetch": true,
      "localStorage": true,
      "atob": true,
      "location": true
    },
    "rules": {
      "react/prop-types": 0,
      "react/jsx-fragments": 0,
      "camelcase": [
        0,
        {
          "ignoreDestructuring": true
        }
      ]
    }
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^Components(.*)$": "<rootDir>/app/components$1",
      "^Containers(.*)$": "<rootDir>/app/containers$1",
      "^Helpers(.*)$": "<rootDir>/app/helpers$1",
      "^Modules(.*)$": "<rootDir>/app/modules$1",
      "^Styles(.*)$": "<rootDir>/app/styles$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "prettier": {
    "semi": false,
    "singleQute": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules"
  ]
}
